generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model LogEvent {
  id         Int      @id @default(autoincrement())
  timestamp  DateTime
  service    String
  level      String
  message    String
  lineNumber Int

  @@index([timestamp])
  @@index([service])
  @@index([level])
}

